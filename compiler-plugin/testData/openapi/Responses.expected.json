                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "X-Rate-Limit-Remaining",
                        "in": "header",
                        "description": "Remaining requests in the current period",
                        "required": false,
                        "schema": {
                            "type": "string"
