        "/results/operation": {
            "get": {
                "summary": "Operation with multiple possible outcomes",
                "parameters": [
                    {
                        "name": "success",
                        "in": "query",
                        "description": "Should the operation succeed",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "default": "true"
