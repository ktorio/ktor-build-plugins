                    },
                    {
                        "name": "includeWarning",
                        "in": "query",
                        "description": "Include warning in response",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "default": "false"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operation succeeded",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SuccessResponse"
                                }
